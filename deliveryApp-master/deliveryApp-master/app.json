{
  "expo": {
    "name": "deliveryApp",
    "slug": "deliveryApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.rafaelfc.deliveryApp",
      "versionCode": 1,
      "permissions": ["ACCESS_FINE_LOCATION"]
    }
  }
}
